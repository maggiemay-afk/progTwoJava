package examPractice;

public class Scope {

	int a;
	public void f1(int x) {
		int b =10;
		
		if (a>10) {
			int e =10;
			b +=x;
			b+=c;
			
			if (a>20) {
				
				int d=10;
				b+=x;
				b+=c;
				b+=d;
				b+=e;
				
			} else {
				int c=10;
				b+=x;
				b+=c;
				b+=d;
				b+=e;
				
			}
		}
		
		b+=x;
		b+=c;
		b=+d;
		b+=e;
	}

}
